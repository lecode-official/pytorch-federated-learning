{
    "editor.rulers": [150],

    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__pycache__": true
    },

    "cSpell.words": [
        "cifar",
        "convolutional",
        "levelname",
        "Mahan",
        "optim",
        "psutil",
        "relu",
        "torchmetrics",
        "torchvision",
        "tqdm"
    ]
}
